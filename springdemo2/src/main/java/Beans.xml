<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="man" class="com.pack.springdemo2.Man">
	<property name="name" value="Rajesh"></property>
	<property name="age" value="32"></property>
</bean>
<bean id="woman" class="com.pack.springdemo2.Woman">
	<property name="name" value="Kiran"></property>
	<property name="age" value="52"></property>
</bean>
</beans>